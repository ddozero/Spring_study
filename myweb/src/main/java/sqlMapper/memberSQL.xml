<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.yeong.member">

	<!-- 회원가입 -->
	<insert id = "memberJoin" parameterType="com.yeong.member.model.MemberDTO">
	insert into jsp_member values (jsp_member_idx.nextval,
									#{id},
									#{password},
									#{name},
									#{email},
									#{tel},
									#{address},
									sysdate)
	
	
	</insert>

</mapper>